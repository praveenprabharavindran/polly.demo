{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RetryConfig": {
    "PertAttemptTimeoutSeconds": 2, // timeout for each attempt (in seconds)
    "MaxRetryAttempts": 2, // desired maximum number of retry attempts
    "RetryDelaySeconds": 1 // desired median delay between retries (in seconds)
  }
}